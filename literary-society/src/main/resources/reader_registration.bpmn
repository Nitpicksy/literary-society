    <bpmn:userTask id="Task_083jyca" name="Unos podataka">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="username" label="Username" type="string">
            <camunda:properties>
              <camunda:property id="errorMessage" value="Username should contain between 4 and 64 alphanumeric characters." />
            </camunda:properties>
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
              <camunda:constraint name="pattern" config="^[A-Za-z0-9]{4,64}$" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="password" label="Password" type="password">
            <camunda:properties>
              <camunda:property id="errorMessage" value="Password should contain between 10 and 64 characters including 1 number, 1 special character, 1 lowercase and 1 uppercase letter." />
            </camunda:properties>
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
              <camunda:constraint name="pattern" config="^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[_#?!@$%^&#38;*-.,:;]).{10,64}$" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="firstName" label="First Name" type="string">
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
              <camunda:constraint name="pattern" config="^(([A-Za-zÀ-ƒ]+[.]?[ ]?|[a-zÀ-ƒ]?[&#39;-]?){0,4})$" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="lastName" label="Last Name" type="string">
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
              <camunda:constraint name="pattern" config="^(([A-Za-zÀ-ƒ]+[.]?[ ]?|[a-zÀ-ƒ]?[&#39;-]?){0,4})$" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="city" label="City" type="string">
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="country" label="Country" type="string">
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="email" label="Email" type="string">
            <camunda:properties>
              <camunda:property id="errorMessage" value="Invalid e-mail address entered." />
            </camunda:properties>
            <camunda:validation>
              <camunda:constraint name="required" config="true" />
              <camunda:constraint name="pattern" config="^[a-z0-9!#$%&#38;&#39;*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&#38;&#39;*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?$" />
            </camunda:validation>
          </camunda:formField>
          <camunda:formField id="selectGenres" label="Genres of interest" type="enum" />
          <camunda:formField id="isBetaReader" label="I also want to be a Beta-reader" type="boolean" />
        </camunda:formData>
